{{ page.highlighted }}

{{ page.help }}

<div class="page--node--evpp-data-visualization">
  <main role="main">
    <a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}
    <div class="layout-content">
      {{ page.content }}
    </div>{# /.layout-content #}
